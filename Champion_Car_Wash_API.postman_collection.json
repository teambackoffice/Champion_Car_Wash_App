					"name": "Login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {",
									"    var jsonData = pm.response.json();",
									"    pm.environment.set(\"sid\", jsonData.message.sid);",
									"    pm.environment.set(\"api_key\", jsonData.message.api_key);",
									"    pm.environment.set(\"branch\", jsonData.message.branch);",
									"    pm.environment.set(\"full_name\", jsonData.full_name);",
									"    console.log(\"‚úÖ Login successful! SID saved to environment.\");",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{base_url}}/api/method/carwash.Api.auth.user_login?usr={{username}}&pwd={{password}}",
							"host": ["{{base_url}}"],
							"path": ["api", "method", "carwash.Api.auth.user_login"],
							"query": [
								{
									"key": "usr",
									"value": "{{username}}",
									"description": "Username or email"
								},
								{
									"key": "pwd",
									"value": "{{password}}",
									"description": "Password"
								}
							]
						},
						"description": "Authenticate user and get session ID (SID) for subsequent requests"
					},
					"response": []
				}
			]
